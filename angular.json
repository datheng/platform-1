    "ipe": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ipe",
      "sourceRoot": "projects/ipe/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ipe",
            "index": "projects/ipe/src/index.html",
            "main": "projects/ipe/src/main.ts",
            "polyfills": "projects/ipe/src/polyfills.ts",
            "tsConfig": "projects/ipe/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/ipe/src/favicon.ico",
              "projects/ipe/src/assets"
            ],
            "styles": [
              "projects/ipe/src/styles.scss",
              "projects/ipe/src/theme.scss",
              "node_modules/prismjs/themes/prism-okaidia.css"
            ],
            "scripts": [
              "node_modules/marked/lib/marked.js",
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components/prism-json.min.js",
              "node_modules/prismjs/components/prism-typescript.min.js",
              "node_modules/prismjs/components/prism-css.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ipe/src/environments/environment.ts",
                  "with": "projects/ipe/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ipe:build",
            "sourceMap": {
              "scripts": true,
              "styles": true,
              "vendor": true
            }
          },
          "configurations": {
            "production": {
              "browserTarget": "ipe:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ipe:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ipe/src/test.ts",
            "polyfills": "projects/ipe/src/polyfills.ts",
            "tsConfig": "projects/ipe/tsconfig.spec.json",
            "karmaConfig": "projects/ipe/karma.conf.js",
            "assets": [
              "projects/ipe/src/favicon.ico",
              "projects/ipe/src/assets"
            ],
            "styles": [
              "projects/ipe/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ipe/tsconfig.app.json",
              "projects/ipe/tsconfig.spec.json",
              "projects/ipe/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ipe/e2e/protractor.conf.js",
            "devServerTarget": "ipe:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ipe:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/ipe/server",
            "main": "projects/ipe/src/main.server.ts",
            "tsConfig": "projects/ipe/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "bundleDependencies": true,
              "fileReplacements": [
                {
                  "replace": "projects/ipe/src/environments/environment.ts",
                  "with": "projects/ipe/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            },
            "dev": {
              "outputHashing": "media",
              "bundleDependencies": true,
              "fileReplacements": [
                {
                  "replace": "projects/ipe/src/environments/environment.ts",
                  "with": "projects/ipe/src/environments/environment.dev.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            },
            "local.dev": {
              "outputHashing": "media",
              "bundleDependencies": true,
              "fileReplacements": [
                {
                  "replace": "projects/ipe/src/environments/environment.ts",
                  "with": "projects/ipe/src/environments/environment.local.dev.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        }
      }
    },
