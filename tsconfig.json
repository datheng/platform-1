{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "attributes": [
        "dist/attributes/attributes",
        "dist/attributes"
      ],
      "material": [
        "dist/material/material",
        "dist/material"
      ],
      "utils": [
        "dist/utils/utils",
        "dist/utils"
      ],
      "content": [
        "dist/content/content",
        "dist/content"
      ],
      "token": [
        "dist/token/token",
        "dist/token"
      ],
      "context": [
        "dist/context/context",
        "dist/context"
      ],
      "style": [
        "dist/style/style",
        "dist/style"
      ],
      "pages": [
        "dist/pages/pages",
        "dist/pages"
      ],
      "media": [
        "dist/media/media",
        "dist/media"
      ],
      "auth": [
        "dist/auth/auth",
        "dist/auth"
      ],
      "plugin": [
        "dist/plugin/plugin",
        "dist/plugin"
      ],
      "datasource": [
        "dist/datasource/datasource",
        "dist/datasource"
      ],
      "layout": [
        "dist/layout/layout",
        "dist/layout"
      ],
      "panels": [
        "dist/panels/panels",
        "dist/panels"
      ],
      "alias": [
        "dist/alias/alias",
        "dist/alias"
      ],
      "pagealias": [
        "dist/pagealias/pagealias",
        "dist/pagealias"
      ],
      "formly": [
        "dist/formly/formly",
        "dist/formly"
      ],
      "rest": [
        "dist/rest/rest",
        "dist/rest"
      ],
      "bridge": [
        "dist/bridge/bridge",
        "dist/bridge"
      ],
      "state": [
        "dist/state/state",
        "dist/state"
      ],
      "durl": [
        "dist/durl/durl",
        "dist/durl"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}